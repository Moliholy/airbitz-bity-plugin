{
  "name": "bity-airbitz-plugin",
  "version": "1.0.0",
  "main": "index.js",
  "license": "LicenseRef-LICENSE",
  "private": true,
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "ci": "run-s lint test dist",
    "lint": "run-s lint:js",
    "lint:js": "eslint .",
    "test": "karma start karma.conf.js --single-run",
    "test:watch": "karma start karma.conf.js --single-run=false",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config ./dev-tools/webpack/webpack.cfg.js",
    "dist": "node ./dev-tools/commands/dist",
    "dist:clean": "shx rm -rf dist",
    "dist:compile": "cross-env NODE_ENV=production webpack -p --progress --config ./dev-tools/webpack/webpack.cfg.js",
    "dist:clean-after-compile": "shx rm ./dist/*.js ./dist/*.css",
    "dist-abc": "node ./dev-tools/commands/dist-abc",
    "dist-abc:move-result": "shx mv ./dist/index.html ./index.html",
    "dist-abc:clear-result": "shx rm -f ./index.html"
  },
  "devDependencies": {
    "autoprefixer": "^6.6.1",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-react-inline-elements": "^6.8.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.11",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "cross-env": "^3.1.4",
    "css-loader": "^0.26.1",
    "cssnano": "^3.10.0",
    "eslint": "^3.13.0",
    "eslint-config-airbnb": "latest",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.9.0",
    "exports-loader": "^0.6.3",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.4",
    "html-webpack-inline-source-plugin": "^0.0.6",
    "html-webpack-plugin": "^2.26.0",
    "image-webpack-loader": "^3.1.0",
    "imports-loader": "^0.7.0",
    "jasmine-core": "^2.5.2",
    "karma": "^1.3.0",
    "karma-jasmine": "^1.1.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-webpack": "^1.8.1",
    "less": "^2.7.2",
    "less-loader": "^2.2.3",
    "node-sass": "^4.2.0",
    "npm-run-all": "^4.0.0",
    "null-loader": "^0.1.1",
    "phantomjs-prebuilt": "^2.1.14",
    "postcss-loader": "^1.2.1",
    "react-hot-loader": "3.0.0-beta.6",
    "sass-loader": "^4.1.1",
    "shx": "^0.2.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "2.2.0",
    "webpack-dev-server": "2.2.0"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "classnames": "^2.2.5",
    "cookies-js": "^1.2.3",
    "libphonenumber-js": "^0.3.14",
    "numeral": "^2.0.4",
    "plugins.airbitz.org": "Airbitz/airbitz-plugins",
    "react": "^15.4.2",
    "react-burger-menu": "^1.10.10",
    "react-dom": "^15.4.2",
    "react-phone-number-input": "^0.8.0",
    "react-redux": "4.4.6",
    "react-redux-form": "^1.5.3",
    "react-router": "^3.0.0",
    "redux": "^3.6.0",
    "redux-saga": "^0.14.3",
    "whatwg-fetch": "^2.0.2"
  }
}
